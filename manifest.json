{
"manifest_version": 1,
    "name": "Freeze Emoji",
    "description": "Stops Animation on Google Chat Custom Emoji",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": [
                "https://chat.google.com/*",
                "https://mail.google.com/chat/*"
            ],
            "all_frames": true,
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
